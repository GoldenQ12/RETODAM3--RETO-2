	<queryString language="SQL">
		<![CDATA[SELECT 
    e.id_empleado,
    e.nombre,
    e.apellidos,
    FORMAT(f.fecha, 'yyyy-MM') AS mes,
    SUM(DATEDIFF(MINUTE, f.hora_entrada, f.hora_salida) / 60.0) AS horas_trabajadas
FROM dbo.fichajes f
JOIN dbo.empleados e ON f.id_empleado = e.id_empleado
WHERE f.hora_entrada IS NOT NULL 
    AND f.hora_salida IS NOT NULL
    AND FORMAT(f.fecha, 'yyyy-MM') = $P{Parameter1}
GROUP BY e.id_empleado, e.nombre, e.apellidos, FORMAT(f.fecha, 'yyyy-MM')
ORDER BY e.id_empleado, mes;]]>
	</queryString>
